{
  "name": "vueify",
  "version": "0.9.0",
  "description": "Starter kit for creating apps with Vue and Vuex",
  "engines": {
    "npm": ">=3"
  },
  "author": {
    "name": "Ivan Novikov",
    "email": "novikov@amiforus.com",
    "url": "https://github.com/jonny-novikov"
  },
  "private": true,
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "watch": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dev": "webpack-dev-server --inline --hot",
    "open:dist": "babel-node tools/distServer.js",
    "clean-dist": "babel-node tools/cleanDist.js",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js",
    "dist": "npm run build && npm run open:dist",
    "ts": "tsc -p ./src",
    "test": "mocha test/testSetup.js \"test/**/*.spec.js\" --reporter progress",
    "test-local": "mocha test/testSetup.js \"test/**/*.test.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"test/**/*.spec.js\" --reporter progress",
    "test:watch": "npm run test -- --watch",
    "eslint": "esw src --color",
    "stylelint": "stylelint ./src/**/*.css --color",
    "lint": "npm run eslint && npm run stylelint",
    "lint:watch": "npm run eslint -- --watch"
  },
  "dependencies": {
    "es6-promise": "3.3.1",
    "vue": "^2.0.3",
    "vue-i18n": "^4.6.0",
    "vue-router": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-loader": "6.2.7",
    "babel-plugin-lodash": "3.2.9",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "browser-sync": "2.17.5",
    "chai": "^3.4.1",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-env": "3.1.3",
    "css-loader": "^0.25.0",
    "eslint": "3.8.1",
    "eslint-plugin-html": "1.5.5",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-vue": "0.1.1",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.2",
    "ismobilejs": "0.4.0",
    "isomorphic-fetch": "2.2.1",
    "isparta": "4.0.0",
    "jsdom": "9.8.3",
    "lodash": "4.16.4",
    "mkdirp": "0.5.1",
    "mocha": "3.1.2",
    "node-sass": "3.10.1",
    "normalize.css": "5.0.0",
    "nouislider": "9.0.0",
    "npm-run-all": "3.1.1",
    "open": "0.0.5",
    "postcss": "5.2.5",
    "postcss-cssnext": "2.8.0",
    "postcss-import": "8.2.0",
    "postcss-loader": "1.1.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "sinon": "1.17.6",
    "style-loader": "0.13.1",
    "stylelint": "7.6.0",
    "stylelint-config-standard": "15.0.0",
    "uglify-loader": "1.3.0",
    "url-loader": "0.5.7",
    "vue-hot-reload-api": "2.0.6",
    "vue-loader": "9.7.0",
    "vuex": "^2.0.0",
    "webpack": "1.13.3",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.1"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue-router",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "vuex",
    "flux",
    "boilerplate",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jonny-novikov/vue-slingshot"
  }
}
